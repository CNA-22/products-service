{
    "name": "products-service",
    "version": "0.1.0",
    "homepage": "https://github.com/CNA-22/products-service#readme",
    "bugs": "https://github.com/CNA-22/products-service/issues",
    "license": "MIT",
    "private": true,
    "contributors": [
        "Alexander Nylund (https://github.com/slightly-blue)",
