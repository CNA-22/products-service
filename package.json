        "Niklas Krogerus (https://github.com/Kraakan)"
    ],
    "repository": "github:CNA-22/products-service",
    "scripts": {
        "start": "ts-node --transpile-only src/index.ts",
        "dev": "tsnd --poll --respawn --transpile-only src/index.ts",
        "tsc": "tsc",
        "lint": "eslint .",
        "fix": "eslint --fix .",
        "prettier": "prettier --check .",
        "format": "prettier --write .",
        "tlp": "npm run tsc && npm run lint && npm run prettier",
