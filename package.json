        "prepare": "husky install",
        "create-token": "./scripts/createToken.js"
    },
    "dependencies": {
        "ajv": "^8.9.0",
        "cloudinary": "^1.28.1",
        "cors": "^2.8.5",
        "express": "^4.17.2",
        "express-jwt": "^6.1.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.1.7",
        "morgan": "^1.10.0",
        "multer": "^1.4.4",
        "path-to-regexp": "^6.2.0",
        "streamifier": "^0.1.1",
        "ts-node": "^10.4.0",
        "tslib": "^2.3.1",
        "typescript": "^4.5.5"
    },
    "devDependencies": {
        "@danielgiljam/eslint-config": "^3.0.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-jwt": "6.0.4",
